<services>
    <service>
        <id>1</id>
        <serviceName>Harambee Members Portal</serviceName>
        <serviceHost>selfservice.harambeesacco.com</serviceHost>
        <servicePort>443</servicePort>
        <serviceResourceURI>/</serviceResourceURI>
        <serviceMethod>GET</serviceMethod>
        <expectedTelnetResponse>Connected to selfservice.harambeesacco.com.</expectedTelnetResponse>
        <expectedRequestResponse>Welcome to Harambee Sacco Society Ltd Members' Portal</expectedRequestResponse>
        <monitoringInterval>10</monitoringInterval>
        <monitoringIntervalTimeUnit>Minutes</monitoringIntervalTimeUnit>
    <enableFileLogging>Yes</enableFileLogging><fileLoggingInterval>Hourly</fileLoggingInterval><enableLogsArchiving>Yes</enableLogsArchiving><logArchivingIntervals>Weekly</logArchivingIntervals></service>
    <service>
        <id>2</id>
        <serviceName>Sky World Services Portal</serviceName>
        <serviceHost>portal.skyworld.co.ke</serviceHost>
        <servicePort>443</servicePort>
        <serviceResourceURI>/</serviceResourceURI>
        <serviceMethod>GET</serviceMethod>
        <expectedTelnetResponse>Connected to portal.skyworld.co.ke.</expectedTelnetResponse>
        <expectedRequestResponse>Welcome to Sky World Limited Services Hub</expectedRequestResponse>
        <monitoringInterval>300</monitoringInterval>
        <monitoringIntervalTimeUnit>Seconds</monitoringIntervalTimeUnit>
    <enableFileLogging>No</enableFileLogging><fileLoggingInterval>Daily</fileLoggingInterval><enableLogsArchiving>No</enableLogsArchiving><logArchivingIntervals>Weekly</logArchivingIntervals></service>
</services>